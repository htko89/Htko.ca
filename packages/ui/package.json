{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "imports": {
    "#src/*": ["./src/*.ts", "./src/*.tsx", "./src/*.d.ts", "./src/*/index.ts", "./src/*/index.tsx", "./src/index.d.ts", "./src/*"]
  },
  "exports": {
    "./*": {
      "types": ["./src/*.ts", "./src/*.tsx", "./src/*.d.ts", "./src/*/index.ts", "./src/*/index.tsx", "./src/index.d.ts", "./src/*"],
      "default": ["./src/*.ts", "./src/*.tsx", "./src/*.d.ts", "./src/*/index.ts", "./src/*/index.tsx", "./src/index.d.ts", "./dist/*"]
    }
  },
  "scripts": {
    "dev": "tailwindcss -i ./src/themes/tailwind.css -o ./dist/themes/tailwind.css --watch",
    "build": "tailwindcss -i ./src/themes/tailwind.css -o ./dist/themes/tailwind.css",
    "lint": "eslint ./src/",
    "lint:fix": "pnpm lint --fix",
    "type:check": "tsc --noEmit",
    "clean": "rimraf ./node_modules"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/react": "^18.2.61",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "postcss": "^8.4.35",
    "prettier": "^3.3.2",
    "react": "^18.2.0",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3"
  }
}