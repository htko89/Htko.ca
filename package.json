{
  "name": "@tkodev/app-tkodev",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.2.1",
  "scripts": {
    "postinstall": "husky install",
    "build": "turbo run build",
    "start": "turbo run start --parallel",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "reset": "turbo run reset --parallel && rimraf ./node_modules && pnpm install"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "rimraf": "^3.0.2",
    "turbo": "^1.3.1"
  }
}
