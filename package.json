{
  "name": "@tkodev/site-dev.tko.www",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "yarn@3.2.1",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "build": "turbo run build",
    "prebuild": "turbo run prebuild",
    "start": "turbo run start --parallel",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "reset": "turbo run reset --parallel && rimraf ./node_modules",
    "image": "docker build -t @tkodev/site-dev.tko.www ."
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "rimraf": "^3.0.2",
    "turbo": "^1.3.1"
  },
  "resolutions": {
    "@types/react": "^17.0.2"
  }
}
