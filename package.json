{
  "name": "@tkodev/site-portfolio",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.2.2",
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "husky install",
    "build": "turbo run build",
    "prebuild": "turbo run prebuild",
    "start": "turbo run start --parallel",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "reset": "turbo run reset --parallel && rimraf ./node_modules"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "rimraf": "^3.0.2",
    "turbo": "^1.3.1"
  },
  "resolutions": {
    "@types/react": "^17.0.2"
  }
}
