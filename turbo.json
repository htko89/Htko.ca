{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": true,
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "start": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "reset": {
      "cache": false
    }
  }
}
